  },
  "scripts": {
    "v:dev": "NODE_ENV=development vite --mode development ",
    "v:build:test": "NODE_ENV=development NODE_OPTIONS=--max-old-space-size=8192 node --max-old-space-size=8192  ./node_modules/vite/bin/vite.js build --mode test ",
    "v:build:dev": "NODE_ENV=development NODE_OPTIONS=--max-old-space-size=8192 node --max-old-space-size=8192  ./node_modules/vite/bin/vite.js build --mode dev ",
    "v:build:pre": "NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 node --max-old-space-size=8192  ./node_modules/vite/bin/vite.js build --mode pre ",
    "v:build": "NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 node --max-old-space-size=8192  ./node_modules/vite/bin/vite.js build --mode production ",
    "start": "npm run v:dev",
    "dev": "npm run v:build:dev",
    "test": "npm run v:build:test",
    "pre": "npm run v:build:pre",
    "build": "npm run v:build",
    "postinstall": "husky install",
    "prestart": "husky install ",
    "build:pre": "REACT_APP_ENV=production craco  build",
    "prebuild:pre": "REACT_APP_ENV=production ./scripts/changeEnv.sh pre",
    "postbuild:pre": "rm -rf ./.env.*.local",
    "test:dev": "REACT_APP_ENV=development craco  build",
    "pretest:dev": "REACT_APP_ENV=development ./scripts/changeEnv.sh dev",
    "posttest:dev": "rm -rf ./.env.*.local",
    "pretest": "REACT_APP_ENV=development ./scripts/changeEnv.sh test",
    "posttest": "rm -rf ./.env.*.local",
    "predev": "REACT_APP_ENV=development ./scripts/changeEnv.sh dev",
    "format": "npm run prettier 'src/**/*.{js,ts, jsx,tsx}' --write && npm run lint:fix",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix ./src --ext .js,.jsx,.ts,.tsx",
    "api2": "ts-node ./sdk-template/script.ts",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@craco/craco": "7.0.0",
    "@dnd-kit/utilities": "^3.2.1",
    "@nabla/vite-plugin-eslint": "^1.5.0",
    "@svgr/webpack": "4.3.2",
    "@types/crypto-js": "^4.1.1",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "24.0.23",
    "@types/lodash-es": "^4.17.6",
    "@types/react-dnd": "^3.0.2",
    "@types/react-dnd-html5-backend": "^3.0.2",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@typescript-eslint/typescript-estree": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "craco-less": "^2.0.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "react-app-alias": "^2.2.2",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^5.0.0",
    "swaggie-beta": "^0.7.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "0.0.1-alpha.0",
    "vite": "4.1.4",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-linter": "^2.0.2",
    "vite-plugin-zip-pack": "^1.0.6",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|less|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "proxy": "http://localhost:3000"
}
